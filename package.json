{
  "name": "piratedb",
  "version": "1.0.21",
  "private": true,
  "scripts": {
    "postinstall": "npm run bundle",
    "dev": "node node_modules/concurrently/src/main.js -k \"npm run server-dev\" \"npm run bundle-watch-live-reload\" \"npm run stylesheets-watch\"",
    "dev-frontend": "node node_modules/concurrently/src/main.js -k \"npm run bundle-watch\" \"npm run stylesheets-watch\"",
    "start": "NODE_ENV=production npm run server",
    "server": "node --harmony ./bin/www.js",
    "server-dev": "node_modules/.bin/nodemon -L --watch ./src/backend --exec \"node --harmony ./bin/www.js\"",
    "server-dev-debug": "node_modules/.bin/nodemon -L --watch ./src/backend --exec \"node --debug --harmony ./bin/www.js\"",
    "pretest": "NODE_ENV=test node_modules/sequelize-cli/bin/sequelize db:migrate:undo:all --config=src/backend/config/sequelize.js && node_modules/sequelize-cli/bin/sequelize db:migrate --config=src/backend/config/sequelize.js",
    "test": "npm run server-tests && npm run component-tests",
    "server-tests": "NODE_ENV=test ./node_modules/mocha/bin/mocha --harmony -r spec/support/globals \"spec/backend/**/*.js\"",
    "component-tests": "NODE_ENV=test node ./node_modules/karma-cli/bin/karma start",
    "db-migrations": "sequelize db:migrate --config=src/backend/config/sequelize.js",
    "bundle": "NODE_ENV=production ./node_modules/.bin/webpack --config ./webpack.config.js --progress --colors -p",
    "bundle-staging": "NODE_ENV=staging ./node_modules/.bin/webpack --config ./webpack.config.js --progress --colors -p",
    "bundle-watch": "./node_modules/.bin/webpack --config ./webpack.dev.config.js --progress --colors --watch",
    "bundle-watch-live-reload": "./node_modules/.bin/webpack-dev-server --config webpack.dev.config.js",
    "stylesheets": "cp ./node_modules/react-mdl/extra/material.min.css public/stylesheets/material.min.css && sass --load-path ./node_modules/bourbon/app/assets/stylesheets --load-path ./node_modules/bourbon-neat/app/assets/stylesheets public/stylesheets/style.scss:public/stylesheets/style.css",
    "stylesheets-watch": "cp ./node_modules/react-mdl/extra/material.min.css public/stylesheets/material.min.css && sass --load-path ./node_modules/bourbon/app/assets/stylesheets --load-path ./node_modules/bourbon-neat/app/assets/stylesheets --watch public/stylesheets/style.scss:public/stylesheets/style.css",
    "manage": "node --harmony ./scripts/manage.js"
  },
